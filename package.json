{
  "name": "data-capsule",
  "version": "1.0.0",
  "author": {
    "name": "Shahar Talmi",
    "email": "shahart@wix.com",
    "url": ""
  },
  "main": "dist/src/index.js",
  "types": "dist/src/typings/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "yoshi start",
    "pretest": "yoshi lint && yoshi build",
    "build": ":",
    "test": "yoshi test",
    "release": "yoshi release; bower-auto-release --dist dist/statics"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "greedy-split": "^1.0.0",
    "message-channel": "^2.0.0"
  },
  "private": false,
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.20.0",
    "bower-auto-release": "^1.1.11",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "describe-jsdom": "^1.1.2",
    "eslint-config-wix": "^1.1.16",
    "express": "^4.16.1",
    "nock": "^9.0.2",
    "node-localstorage": "^1.3.0",
    "protractor": "^5",
    "sinon": "^1.17.7",
    "yoshi": "^3"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-runtime"
        ],
        "presets": [
          "stage-0",
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "development": {
        "presets": [
          "stage-0",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 3 versions",
                  "ie >= 11",
                  "Safari >= 8"
                ]
              }
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "wix/esnext"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "yoshi": {
    "entry": {
      "index": "./global.js",
      "frame": "./global-frame.js",
      "frame-listener": "./global-frame-listener"
    },
    "exports": "data-capsule"
  }
}
